{
    // cspell:words Sysnative windir
    // cspell:words Cartograph Consolas dprint Nushell stylua
    // cspell:words cheshirekow jnoortheen JohnnyMorganz
    "[lua]": {
        "editor.defaultFormatter": "JohnnyMorganz.stylua"
    },
    "[json][jsonc][markdown][toml]": {
        "editor.defaultFormatter": "dprint.dprint"
    },
    "[nix]": {
        "editor.defaultFormatter": "jnoortheen.nix-ide"
    },
    "nix.enableLanguageServer": true,
    "nix.serverPath": "nil",
    "nix.serverSettings": {
        "nil": {
            "formatting": {
                "command": [
                    "nixfmt"
                ]
            }
        }
    },
    "editor.fontFamily": "Cartograph CF, Consolas, 'Courier New', monospace",
    "editor.rulers": [
        100
    ],
    "terminal.integrated.commandsToSkipShell": [
        "-workbench.action.quickOpen"
    ],
    "terminal.integrated.defaultProfile.windows": "NuShell",
    "terminal.integrated.profiles.windows": {
        "Command Prompt": {
            "args": [],
            "icon": "terminal-cmd",
            "path": [
                "${env:windir}\\Sysnative\\cmd.exe",
                "${env:windir}\\System32\\cmd.exe"
            ]
        },
        "JavaScript Debug Terminal": null,
        "Nushell": null,
        "NuShell": {
            "path": [
                "${env:SCOOP}\\shims\\nu.exe"
            ]
        },
        "PowerShell": {
            "icon": "terminal-powershell",
            "source": "PowerShell"
        }
    },
    "extensions.ignoreRecommendations": true,
    "redhat.telemetry.enabled": false,
    "editor.formatOnSave": true,
    "remote.SSH.remotePlatform": {
        "mustafar": "linux",
        "geonosis": "linux"
    },
    "json.schemas": [
        {
            "fileMatch": [
                "CMakePresets.json",
                "CMakeUserPresets.json"
            ],
            "url": "https://cmake.org/cmake/help/latest/_downloads/3e2d73bff478d88a7de0de736ba5e361/schema.json"
        }
    ],
    "cmake.languageSupport.enableFileAPI": true,
    "[cmake]": {
        "editor.defaultFormatter": "cheshirekow.cmake-format"
    },
    "workbench.sideBar.location": "right",
    "workbench.settings.applyToAllProfiles": [],
    "gitlab.aiAssistedCodeSuggestions.enabled": false,
    "gitlab.duoChat.enabled": false,
    "window.zoomLevel": 1,
    "files.associations": {
        ".vscode/*.json": "jsonc",
        "*.just": "just",
        "dep5": "debian-copyright",
        "*.json5": "jsonc",
        "COMMIT_EDITMSG-*": "git-commit",
    },
    "yaml.customTags": [
        "!reference sequence"
    ],
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "explorer.fileNesting.enabled": true,
    "explorer.fileNesting.expand": false,
    "explorer.fileNesting.patterns": {
        "*": "$(capture).license",
        "*.c": "$(capture).h",
        "*.cc": "$(capture).hpp, $(capture).h",
        "*.cpp": "$(capture).hpp, $(capture).h",
        "*.css": "$(capture).css.map, $(capture).*.css",
        "*.cxx": "$(capture).hpp, $(capture).h",
        "*.go": "$(capture)_test.go",
        "*.java": "$(capture).class",
        "*.tex": "$(capture).acn, $(capture).acr, $(capture).alg, $(capture).aux, $(capture).bbl, $(capture).blg, $(capture).fdb_latexmk, $(capture).fls, $(capture).glg, $(capture).glo, $(capture).gls, $(capture).idx, $(capture).ind, $(capture).ist, $(capture).lof, $(capture).log, $(capture).lot, $(capture).out, $(capture).pdf, $(capture).synctex.gz, $(capture).toc, $(capture).xdv",
        ".clang-tidy": ".clang-format, .clangd, compile_commands.json",
        ".env": "*.env, .env.*, .envrc, env.d.ts",
        ".gitignore": ".gitattributes, .gitmodules, .gitmessage, .mailmap, .git-blame*",
        "CMakeLists.txt": "*.cmake, *.cmake.in, .cmake-format.yaml, CMakePresets.json, CMakeCache.txt",
        "Cargo.toml": ".clippy.toml, .rustfmt.toml, cargo.lock, clippy.toml, cross.toml, rust-toolchain.toml, rustfmt.toml",
        "Dockerfile": "*.dockerfile, .devcontainer.*, .dockerignore, compose.*, docker-compose.*, dockerfile*",
        "README*": "AUTHORS, Authors, BACKERS*, Backers*, CHANGELOG*, CITATION*, CODEOWNERS, CODE_OF_CONDUCT*, CONTRIBUTING*, CONTRIBUTORS, COPYING*, CREDITS, Changelog*, Citation*, Code_Of_Conduct*, Codeowners, Contributing*, Contributors, Copying*, Credits, GOVERNANCE.MD, Governance.md, HISTORY.MD, History.md, LICENSE*, License*, MAINTAINERS, Maintainers, README*, Readme*, SECURITY.MD, SPONSORS*, Security.md, Sponsors*, authors, backers*, changelog*, citation*, code_of_conduct*, codeowners, contributing*, contributors, copying*, credits, governance.md, history.md, license*, maintainers, readme*, security.md, sponsors*, .editorconfig",
        "default.nix": "shell.nix",
        "flake.nix": "flake.lock, .envrc",
    },
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/.direnv": true,
        "**/.gitlab-ci-local": true
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "workbench.colorTheme": "Gruvbox Dark Medium",
    "workbench.layoutControl.enabled": false
}
